package principal;

public class Program {

	public static void main(String[] args) {
		
		int valor1 = 50;
		int valor2 = 65;
		
		//final sinaliza que a variavel é uma constante
		//uma constante é quando o valor nao muda
		final double PI = 3.1415;
		
		//realiza a potencia/ primeiro é a base e depois a potencia
		System.out.println(Math.pow(2, 2));
		
		//passsagem de 2 numeros como parametro
		//retona o maior deles
		System.out.println(Math.max(valor1, valor2));
		
		//É o inverso do maior, retorna o minimo
		System.out.println(Math.min(5, 20));
		
		//retorna a raiz quadrada e um numero
		System.out.println(Math.sqrt(18));
		
		//arredonda esse numero (para mais ou menos)
		System.out.println(Math.round(25.4578));
	
		
		
	}

}
